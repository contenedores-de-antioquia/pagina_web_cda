{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "productName": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "productName"
    },
    "newOrUsed": {
      "type": "enumeration",
      "enum": [
        "Contenedor nuevo",
        "Contenedor usado"
      ]
    },
    "mainImage": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "container_category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::container-category.container-category"
    },
    "saleValue": {
      "type": "decimal"
    },
    "rentalValue": {
      "type": "decimal"
    },
    "active": {
      "type": "boolean",
      "default": true
    },
    "imageMeasurements": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "typeOfCoating": {
