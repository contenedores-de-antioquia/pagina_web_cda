{
  "kind": "collectionType",
  "collectionName": "container_categories",
  "info": {
    "singularName": "container-category",
    "pluralName": "container-categories",
    "displayName": "containerCategory"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nameCategoryContainers": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "nameCategoryContainers"
    },
    "imgContainerCategory": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "inversedBy": "container_categories"
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category"
    }
  }
}
