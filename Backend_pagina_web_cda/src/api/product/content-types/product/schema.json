        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "numberOfWindows": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "doors": {
